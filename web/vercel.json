{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm ci",
  "framework": "vite",
  "outputDirectory": "dist",
  "env": [
    "VITE_BACKEND_URL",
    "VITE_USE_BACKEND_API",
