    "@event-aggregator/shared/runtime/": "./functions/packages/shared/dist/runtime/",
    "@event-aggregator/shared/config/": "./functions/packages/shared/dist/config/",
    "@event-aggregator/shared/validation/": "./functions/packages/shared/dist/validation/",
