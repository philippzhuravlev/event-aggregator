{
  "name": "@event-aggregator/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/node/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/node/index.cjs"
    },
    "./config": {
      "types": "./dist/esm/config/index.d.ts",
      "import": "./dist/esm/config/index.js",
      "require": "./dist/node/config/index.cjs"
    },
    "./config/*": {
      "types": "./dist/esm/config/*.d.ts",
      "import": "./dist/esm/config/*.js",
      "require": "./dist/node/config/*.cjs"
    },
    "./runtime": {
      "types": "./dist/esm/runtime/index.d.ts",
      "import": "./dist/esm/runtime/index.js",
      "require": "./dist/node/runtime/index.cjs"
    },
    "./runtime/node": {
      "types": "./dist/esm/runtime/node.d.ts",
      "import": "./dist/esm/runtime/node.js",
      "require": "./dist/node/runtime/node.cjs"
    },
    "./runtime/deno": {
      "types": "./dist/esm/runtime/deno.d.ts",
      "import": "./dist/esm/runtime/deno.js",
      "default": "./dist/esm/runtime/deno.js"
    },
    "./runtime/browser": {
      "types": "./dist/esm/runtime/browser.d.ts",
      "import": "./dist/esm/runtime/browser.js",
      "default": "./dist/esm/runtime/browser.js"
    },
    "./routing": {
      "types": "./dist/esm/routing.d.ts",
      "import": "./dist/esm/routing.js",
      "require": "./dist/node/routing.cjs"
    },
    "./types": {
      "types": "./dist/esm/types.d.ts",
      "import": "./dist/esm/types.js",
      "require": "./dist/node/types.cjs"
    },
    "./validation": {
      "types": "./dist/esm/validation/index.d.ts",
      "import": "./dist/esm/validation/index.js",
      "require": "./dist/node/validation/index.cjs"
    },
    "./validation/*": {
      "types": "./dist/esm/validation/*.d.ts",
      "import": "./dist/esm/validation/*.js",
      "require": "./dist/node/validation/*.cjs"
    }
  },
  "scripts": {
    "build": "npm run clean && tsup --config tsup.config.ts",
    "clean": "rimraf dist && rimraf ../supabase/packages/shared/dist"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "tsup": "^8.0.1",
    "typescript": "^5.6.3"
  }
}

